<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lambdaschool.androidanimatedimages">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_computer"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_computer_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.AnimatedVectorActivity"></activity>
        <activity android:name=".activity.AnimatedDrawableActivity" />
        <activity android:name=".activity.GifActivity" />
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>